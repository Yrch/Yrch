{% extends 'YrchBundle::layout.twig' %}

{% block assets %}
    {{ parent() }}
    {% stylesheet 'bundles/yrch/css/columns.css' %}
{% endblock %}
{% block container %}
    <div id="content" class="column">
        {% block content %}{% endblock %}
    </div>
    <div id="categories" class="column">
        {% block categories %}{% endblock %}
    </div>
    <div id="right_bar" class="column">
        {% block right_bar %}{% endblock %}
    </div>
{% endblock %}
